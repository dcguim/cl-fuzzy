;;examples of simple modifiers
;;; (not (slightly A))
(let ((a '((0 1/9) (1 2/5) (2 7/16) (3 15/17))))
	      (no (slightly a)))

;;;(extremely (not A))
(let ((a '((0 1/9) (1 2/5) (2 7/16) (3 15/17))))
	      (extremely (no a)))

;;; (little (not A))
 (let ((a '((0 1/9) (1 2/5) (2 7/16) (3 15/17))))
	      (little (no a)))

;;;(extremely (little A))
(let ((a '((0 1/9) (1 2/5) (2 7/16) (3 15/17))))
	      (extremely (little a)))
